    "prepublish": "npm run build; npm run copy-executable"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.18",
    "joi": "^10.4.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "peerDependencies": {
    "ts-node": "^3.0.4"
  }
}
