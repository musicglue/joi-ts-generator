{
  "name": "@musicglue/joi-ts-generator",
  "version": "1.2.1",
  "description": "Generate Typescript Types from Joi schemas",
  "bin": {
    "joi-ts-gen": "dist/index.js"
  },
  "main": "dist/index.js",
  "repository": "https://github.com/musicglue/joi-ts-generator",
  "author": "Music Glue",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "copy-executable": "mkdir -p dist && cp build/index.js dist/index.js",
