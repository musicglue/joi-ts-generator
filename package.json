{
  "name": "@musicglue/joi-ts-generator",
  "version": "4.4.3",
  "description": "Generate Typescript Types from Joi schemas",
  "bin": {
    "joi-ts-gen": "dist/cli.js"
  },
  "main": "dist/index.js",
  "repository": "https://github.com/musicglue/joi-ts-generator",
  "author": "Music Glue",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "copy-templates": "cp -R src/templates dist",
    "build": "npm run clean && tsc && npm run copy-templates",
    "make-executable": "chmod +x dist/cli.js",
    "prepublish": "npm run build; npm run make-executable"
  },
  "devDependencies": {
    "@types/joi": "^10.3.2",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.18",
    "@types/rosie": "^0.0.30",
    "joi": "^10.4.2",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "deep-freeze-strict": "^1.1.1",
    "fp-ts": "^0.2.9",
    "lodash": "^4.17.4",
    "read-pkg-up": "^2.0.0",
    "rosie": "^1.6.0"
  },
  "peerDependencies": {
    "ts-node": "^3.0.4"
  }
}
